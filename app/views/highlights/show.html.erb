<% content_for :head do %>
    <p id="notice"><%= notice %></p>
<% end %>

<% content_for :main do %>
    <p>
      <b>Job ID:</b>
      <%= @highlight.job_id %>
    </p>

    <p>
      <b>Skill ID:</b>
      <%= @highlight.skill_id %>
    </p>

    <p>
      <b>Description:</b>
      <%= @highlight.description %>
    </p>


    <%= link_to 'Edit', edit_highlight_path(@highlight) %> |
    <%= link_to 'Back', highlights_path %>
<% end %>