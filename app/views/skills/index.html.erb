<% content_for :stylesheets do %>
    <style>
        .edit_user_skill {
            padding-right: 28px;
        }
        .edit_user_skill.busy {
            background: url('data:image/gif;base64,') no-repeat right top;
        }
        .edit_user_skill.success {
            background: url('data:image/gif;base64,') no-repeat right top;
        }
    </style>
<% end %>

<% content_for :head do %>
    <hgroup>
        <h1>Skills<% if !@job.nil? %> used at this job<% elsif !@year.nil? %> used in <%= @year.value %><% end %></h1>
    </hgroup>
<% end %>

<% content_for :main do %>
    <section class="primary extended">
        <%= render :partial => 'skills/full_list', :locals => {:user => @user, :resume => @resume, :skills => @skills } %>
    </section>
<% end %>

<% content_for :body_scripts do %>
    <script>
        require(['top/submit_on_change'], function(SubmitOnChange) {
            SubmitOnChange.init(".select-discipline-for-skill", true);
        });
    </script>
<% end %>