<% content_for :head do %>
    <hgroup>
        <h1>Skills<% if !@job.nil? %> used at this job<% elsif !@year.nil? %> used in <%= @year.value %><% end %></h1>
    </hgroup>
<% end %>

<% content_for :main do %>
    <section class="primary extended">
        <%= render :partial => 'skills/full_list', :locals => {:user => @user, :resume => @resume, :skills => @skills } %>
    </section>
<% end %>